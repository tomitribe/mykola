{
  "name": "angular-tomitribe-components",
  "version": "0.1.2",
  "description": "Angular UI Component library named after famous Ukrainian composer, Mykola Leontovych",
  "repository": "https://github.com/tomitribe/mykola.git",
  "main": "/",
  "scripts": {
    "start": "cross-env NODE_ENV=dev webpack-dev-server --config webpack/webpack.dev.js --watch --cache=false --mode=development",
    "build": "cross-env NODE_ENV=production webpack --config  webpack/webpack.dev.js --mode=production",
    "doc": "cross-env NODE_ENV=bundle gulp ngdocs",
    "bundle": "cross-env NODE_ENV=bundle webpack --config  webpack/webpack.dev.js --mode=production",
    "types": "tsc components/ts-tomitribe-util --emitDeclarationOnly=true"
  },
  "author": "Andrii M.",
  "license": "MIT",
  "dependencies": {
    "angular": "1.7.8",
    "angular-cookies": "1.7.8",
    "angular-resource": "1.7.8",
    "angular-route": "1.7.8",
    "angular-sanitize": "1.7.8",
    "bootstrap": "4.3.1",
    "bootstrap-admin-ui": "1.2.0",
    "bourbon": "5.1.0",
    "font-awesome": "4.7.0",
    "html5sortable": "0.9.16",
    "jquery": "3.4.1",
    "ng-infinite-scroll": "1.3.0",
    "selectize": "0.12.6",
    "ui-select": "0.19.8",
    "underscore": "1.9.1"
  },
  "devDependencies": {
    "@types/angular": "1.6.54",
    "@types/core-js": "2.5.2",
    "@types/jquery": "3.3.29",
